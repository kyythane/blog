{
  "name": "blog",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "task": "node --loader ts-node/esm ./scripts/tasks.ts"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "**/*.(ts|js)": [
      "eslint --cache --fix",
      "prettier --write --ignore-unknown"
    ],
    "**/*.svelte": [
      "eslint --cache --fix",
      "prettier --write --ignore-unknown",
      "stylelint --fix"
    ],
    "**/*.svelte.md": [
      "markdownlint --fix"
    ],
    "**/*.css": [
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@atomictech/rehype-toc": "^3.1.3",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/jit": "^0.1.18",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.5",
    "cssnano": "^5.0.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-svelte3": "^3.1.0",
    "front-matter": "^4.0.2",
    "hastscript": "^6.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "markdownlint-cli": "^0.27.1",
    "mdsvex": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-load-config": "^3.0.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-katex": "^4.0.0",
    "rehype-parse": "^7.0.1",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-containers": "^1.2.0",
    "remark-footnotes": "^2.0.0",
    "remark-gemoji": "^6.0.0",
    "remark-math": "^3.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "svelte": "^3.29.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^2.1.1",
    "tasksfile": "^5.1.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "unified": "^9.2.1",
    "vite": "^2.1.0"
  },
  "type": "module",
  "dependencies": {
    "markdownlint": "^0.23.1"
  }
}
